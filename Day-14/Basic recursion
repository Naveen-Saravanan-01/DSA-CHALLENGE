1.Print 1 to N..............
class Solution
{
    
  public void printNos(int N)
    {
        //Your code here
        if(N<1)
        return;
        
        printNos(N-1);
   
         System.out.print(N+" ");

    }
    
}


2.Print name N times............



class Solution {

    void printGfg(int N) {
        // code here
        
        if(N<1) return;
        printGfg(N-1);
        System.out.print("GFG"+" ");
        
    }
}
