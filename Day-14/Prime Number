

class Solution{
    static int isPrime(int N){
        
    
        // code
        if(N==0 || N==1){
            return 0;
        }
        int i=2;
        while(i<=Math.sqrt(N)){
            if(N%i==0){
                return 0;
            }
            i++;
        }
        return 1;
        
        
        
    }
}
